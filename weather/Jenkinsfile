pipeline {
    // agent {
    //     label ("aws-deploy")
    //         }
  agent any
  environment {
		DOCKERHUB_CREDENTIALS=credentials('github-ssh')
	}
    options {
        buildDiscarder(logRotator(numToKeepStr: '5'))
        disableConcurrentBuilds()
        timeout (time: 60, unit: 'MINUTES')
        timestamps()
      }
}






